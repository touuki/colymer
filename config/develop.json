{
    "mongodb": {
        "url": "mongodb://192.168.30.1:27017/",
        "db": "colymer",
        "options": {
            "authSource": "colymer",
            "auth": {
                "user": "colymer",
                "password": "colymer"
            },
            "poolSize": 5,
            "useUnifiedTopology": true
        }
    },
    "storage": "default",
    "default_storage_options": {
        "tmp_dir": "tmp/",
        "api_prefix": "http://localhost:3000/api/",
        "url_prefix": "/attachment",
        "directory": "public/attachment",
        "max_file_size": "1GB"
    },
    "downloader_timeout": 300,
    "downloader":{
        "api_prefix": "http://localhost:3000/api/",
        "keep_alive": 15,
        "download_interval": 0,
        "check_interval": 120,
        "parallel_count": 5,
        "options": {
            "proxy": "http://localhost:7070",
            "headers":{
                "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/87.0.4280.88 Safari/537.36"
            }
        }
    }
}